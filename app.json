{
  "expo": {
    "name": "ShopFun",
    "slug": "ShopFun",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/bl.gif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "Jersey": ["/assets/fonts/Jersey10Charted-Regular.ttf"],
          "Platypi": ["/assets/fonts/Platypi-VariableFont_wght.ttf"]
        }
      ]
    ]
  }
}
